{
  "$schema": "https://unpkg.com/wrangler@latest/config-schema.json",
  "name": "mcpidp-chat",
  "main": "main.ts",
  "compatibility_date": "2025-09-21",
  "assets": { "directory": "./public" },
  "durable_objects": {
    "bindings": [{ "name": "MCPProviders", "class_name": "MCPProviders" }]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["MCPProviders"]
    }
  ],
  "routes": [{ "custom_domain": true, "pattern": "completions.mcpidp.com" }]
}
